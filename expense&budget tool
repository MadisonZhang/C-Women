      "cell_type": "code",
      "source": [
        "#Data visualization\n",
        "#Bar chart: expense month-over-month\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "monthly_expenses = expense_manager.get_monthly_expense()\n",
        "\n",
        "months = list(monthly_expenses.keys())\n",
        "if len(months) >= 2:\n",
        "    month1 = months[0]\n",
        "    month2 = months[1]\n",
        "else:\n",
        "    print(f\"At least two months are needed to compare expenses.\")\n",
        "    exit()\n",
        "\n",
        "def get_category_expenses(month):\n",
        "    categories = []\n",
        "    expenses = []\n",
        "    for category, total_amount in monthly_expenses[month].items():\n",
        "        categories.append(category)\n",
        "        expenses.append(total_amount)\n",
        "    return categories, expenses\n",
        "\n",
        "Ycategories, Yvals = get_category_expenses(month1)\n",
        "Zcategories, Zvals = get_category_expenses(month2)\n",
        "\n",
        "categories = list(set(Ycategories + Zcategories))\n",
        "X_axis = np.arange(len(categories))\n",
        "\n",
        "# Align categories for both months\n",
        "Y_expenses = [Yvals[Ycategories.index(category)] if category in Ycategories else 0 for category in categories]\n",
        "Z_expenses = [Zvals[Zcategories.index(category)] if category in Zcategories else 0 for category in categories]\n",
        "\n",
        "plt.bar(X_axis - 0.2, Y_expenses, 0.4, label=str(month1))\n",
        "plt.bar(X_axis + 0.2, Z_expenses, 0.4, label=str(month2))\n",
        "\n",
        "plt.xticks(X_axis, categories, rotation=45)\n",
        "plt.xlabel(\"Category\")\n",
        "plt.ylabel(\"Amount ($)\")\n",
        "plt.title(\"Expense by category month-over-month\")\n",
        "plt.legend()\n",
        "\n",
        "plt.show()"
