      "source": [
        "#pie chart: expense breakdown\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "monthly_expenses = expense_manager.get_monthly_expense()\n",
        "\n",
        "months = list(monthly_expenses.keys())\n",
        "if len(months) >= 2:\n",
        "    month1 = months[0]\n",
        "    month2 = months[1]\n",
        "else:\n",
        "    print(f\"At least two months are needed to compare expenses.\")\n",
        "    exit()\n",
        "\n",
        "def get_category_expenses(month):\n",
        "    categories = []\n",
        "    expenses = []\n",
        "    for category, total_amount in monthly_expenses[month].items():\n",
        "        categories.append(category)\n",
        "        expenses.append(total_amount)\n",
        "    return categories, expenses\n",
        "\n",
        "Ycategories, Yvals = get_category_expenses(month1)\n",
        "Zcategories, Zvals = get_category_expenses(month2)\n",
        "\n",
        "# Set the figure size\n",
        "plt.figure(figsize=(12, 6))\n",
        "\n",
        "# Plotting pie chart for month 1\n",
        "plt.subplot(1, 2, 1)  # Create a subplot for the first pie chart\n",
        "plt.pie(Yvals, labels=Ycategories, autopct='%1.1f%%', radius=0.8)  # Increase the radius to make the pie larger\n",
        "plt.title(f\"Expense Breakdown - {month1}\")\n",
        "plt.legend(title=\"Categories\", loc=\"center left\", bbox_to_anchor=(1, 0.5))  # Add legend box\n",
        "\n",
        "# Plotting pie chart for month 2\n",
        "plt.subplot(1, 2, 2)  # Create a subplot for the second pie chart\n",
        "plt.pie(Zvals, labels=Zcategories, autopct='%1.1f%%', radius=0.8)  # Increase the radius to make the pie larger\n",
        "plt.title(f\"Expense Breakdown - {month2}\")\n",
        "plt.legend(title=\"Categories\", loc=\"center left\", bbox_to_anchor=(1, 0.5))  # Add legend box\n",
        "\n",
        "plt.tight_layout()  # Adjust spacing between subplots\n",
        "\n",
        "plt.show()"
