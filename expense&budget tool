        "print()\n",
        "\n",
        "# Compare monthly budgets and expenses\n",
        "print(\"***********Monthly Budget vs Expenses***********\")\n",
        "budgets = budget_manager.get_budget()\n",
        "monthly_expenses = expense_manager.get_monthly_expense()\n",
        "\n",
        "print(\"Monthly Expenses:\")\n",
        "for month, categories in monthly_expenses.items():\n",
        "    for category, amount in categories.items():\n",
        "        print(f\"Month: {month} - Category: {category} - Total expense amount: {amount}\")\n",
        "\n",
        "print()\n",
        "\n",
        "if budgets and monthly_expenses:\n",
        "    for month, categories in budgets.items():\n",
        "        print(f\"Month: {month}\")\n",
        "        print(\"Budgets:\")\n",
        "        for category, budget_amount in categories.items():\n",
        "            expense_amount = monthly_expenses.get(month, {}).get(category, 0)\n",
        "            print(f\"{category}: Budget - {budget_amount}, Expense - {expense_amount}\")\n",
        "            remaining = budget_amount - expense_amount\n",
        "            if remaining <50 and remaining >0:\n",
        "              print(f\"Alert: your remaining spendable amount in {category} for {month} is less tha $50.\")\n",
        "            elif remaining <0:\n",
        "              print(f\"Warning: your spending in  {category} for {month} is over budget.\")\n",
        "            else:\n",
        "              print(f\"Your remaining spendable amount in {category} for {month} is {remaining}\")\n",
        "        print()\n"
