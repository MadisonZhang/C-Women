        "#Budgets:\n",
        "import sqlite3\n",
        "\n",
        "class Budget_Manager:\n",
        "  def __init__(self, budget_db_file):\n",
        "    self.conn = sqlite3.connect(budget_db_file)\n",
        "    self.create_table_budgets()\n",
        "\n",
        "  def create_table_budgets(self):\n",
        "    query = \"\"\"\n",
        "    CREATE TABLE IF NOT EXISTS budgets (\n",
        "      id INTEGER PRIMARY KEY AUTOINCREMENT,\n",
        "      month TEXT,\n",
        "      category TEXT,\n",
        "      amount REAL,\n",
        "      expense REAL DEFAULT 0,\n",
        "      UNIQUE(month, category)\n",
        "    )\n",
        "    \"\"\"\n",
        "    self.conn.execute(query)\n",
        "    self.conn.commit()\n",
        "\n",
        " # Method to get updated budgets:\n",
        "  def get_budget(self, budget_id=None):\n",
        "    if budget_id is None:\n",
        "      cursor = self.conn.execute(\"SELECT month, category, amount FROM budgets\")\n",
        "    else:\n",
        "      cursor = self.conn.execute(\"SELECT month, category, amount FROM budgets WHERE id=?\",(budget_id,))\n",
        "    results = cursor.fetchall()\n",
        "\n",
        "    if results:\n",
        "      budgets = {}\n",
        "      for result in results:\n",
        "          month = result[0]\n",
        "          category = result[1]\n",
        "          amount = result[2]\n",
        "\n",
        "          if month not in budgets:\n",
        "            budgets[month] = {}\n",
        "\n",
        "          budgets[month][category] =  amount\n",
        "\n",
        "      return budgets\n",
        "\n",
        "    return None\n",
        "\n",
        "# Method to set budgets:\n",
        "  def save_budget(self, month, category, amount):\n",
        "    self.conn.execute(\n",
        "        \"\"\"\n",
        "        INSERT INTO budgets (month, category, amount)\n",
        "        VALUES(?, ?, ?)\n",
        "        ON CONFLICT (month, category)\n",
        "        DO UPDATE SET amount = excluded.amount\n",
        "        \"\"\",\n",
        "        (month, category, amount),\n",
        "    )\n",
        "    self.conn.commit()\n",
        "    print(f\"Budget saved for {month}: {category} - {amount}\")\n",
        "\n",
        "  #Method to modify budgets:\n",
        "  def modify_budgets(self, budget_id, new_category=None, new_month=None, new_amount=None):\n",
        "    query = \"UPDATE budgets SET\"\n",
        "    params = []\n",
        "\n",
        "    if new_category is not None:\n",
        "      query += \" category=?\"\n",
        "      params.append(new_category)\n",
        "\n",
        "    if new_month is not None:\n",
        "      query += \" month=?\"\n",
        "      params.append(new_month)\n",
        "\n",
        "    if new_amount is not None:\n",
        "      query += \" amount=?\"\n",
        "      params.append(new_amount)\n",
        "\n",
        "    query = query.rstrip(',') + \"WHERE id=?\"\n",
        "    params.append(budget_id)\n",
        "\n",
        "    self.conn.execute(query,tuple(params))\n",
        "    self.conn.commit()\n",
        "\n",
        "    updated_budget = self.get_budget(budget_id)\n",
        "    if updated_budget:\n",
        "      for month, categories in updated_budget.items():\n",
        "        for category, amount in categories.items():\n",
        "          print(f\"Budget updated for {month}: {category} - {amount}.\")\n",
        "\n",
        "    else:\n",
        "      print(\"No budgets found for the specified ID.\")\n",
        "\n",
        "# Method to delete budgets:\n",
        "  def delete_budgets(self, budget_id):\n",
        "    query = \"DELETE FROM budgets WHERE id=?\"\n",
        "    self.conn.execute(query, (budget_id,))\n",
        "    self.conn.commit()\n",
        "    print(f\"Budget {budget_id} successfully deleted.\")\n",
        "    print()\n",
        "\n",
        "    cursor = self.conn.cursor()\n",
        "    cursor.close()\n",
        "\n",
        "    # Fetch and display updated budget details\n",
        "    print(\"***********View budget****************\")\n",
        "    budgets = self.get_budget()\n",
        "    if budgets:\n",
        "        for month, categories in budgets.items():\n",
        "          for category, amount in categories.items():\n",
        "            print(f\"Month: {month}, Category: {category}, Amount: {amount}\")\n",
        "    else:\n",
        "        print(\"No budgets found.\")\n"
      ],
      "metadata": {
