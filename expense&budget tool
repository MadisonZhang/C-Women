      "source": [
        "#Expenses:\n",
        "import sqlite3\n",
        "\n",
        "class Exp_Manager:\n",
        "  def __init__(self, expense_db_file):\n",
        "    self.conn = sqlite3.connect(expense_db_file)\n",
        "    self.create_table_expenses()\n",
        "\n",
        "  def create_table_expenses(self):\n",
        "    query = \"\"\"\n",
        "    CREATE TABLE IF NOT EXISTS expenses(\n",
        "      id INTEGER PRIMARY KEY AUTOINCREMENT,\n",
        "      amount REAL,\n",
        "      desc TEXT,\n",
        "      category TEXT,\n",
        "      date TEXT\n",
        "    )\n",
        "    \"\"\"\n",
        "    self.conn.execute(query)\n",
        "    self.conn.commit()\n",
        "\n",
        "# Method to add expenses:\n",
        "  def save_expense(self, amount, category, desc, date):\n",
        "    self.conn.execute(\n",
        "        \"\"\"\n",
        "        INSERT INTO expenses (amount, category, desc, date)\n",
        "        VALUES (?, ?, ?, ?)\n",
        "        \"\"\",\n",
        "        (amount, category, desc, date)\n",
        "    )\n",
        "    self.conn.commit()\n",
        "    print(f\"Expense added: Amount - {amount}, Desc - {desc}, Category - {category}, Date - {date}\")\n",
        "\n",
        "# Method to modify expense:\n",
        "  def modify_expense(self, exp_id, new_amount=None, new_category=None, new_desc=None, new_date=None):\n",
        "    query = \"UPDATE expenses SET\"\n",
        "    params = []\n",
        "\n",
        "    if new_amount is not None:\n",
        "      query += \" amount=?,\"\n",
        "      params.append(new_amount)\n",
        "\n",
        "    if new_category is not None:\n",
        "      query += \" category=?,\"\n",
        "      params.append(new_category)\n",
        "\n",
        "    if new_desc is not None:\n",
        "      query += \" desc=?,\"\n",
        "      params.append(new_desc)\n",
        "\n",
        "    if new_date is not None:\n",
        "      query += \" date=?,\"\n",
        "      params.append(new_date)\n",
        "\n",
        "    query = query.rstrip(',') + \"WHERE id=?\"\n",
        "    params.append(exp_id)\n",
        "\n",
        "    self.conn.execute(query,tuple(params))\n",
        "    self.conn.commit()\n",
        "\n",
        "    updated_expense = self.get_expense(exp_id)\n",
        "    if updated_expense:\n",
        "        print(f\"Expense updated: ID - {exp_id}, New Amount - {updated_expense[0]['amount']}, \"\n",
        "              f\"New Desc - {updated_expense[0]['desc']}, New Category - {updated_expense[0]['category']}, \"\n",
        "              f\"New Date - {updated_expense[0]['date']}.\")\n",
        "    else:\n",
        "        print(\"No expense found for the specified ID.\")\n",
        "\n",
        "# Method to delete expense:\n",
        "  def delete_expense(self,exp_id):\n",
        "    query = \"DELETE FROM expenses WHERE id=?\"\n",
        "    self.conn.execute(query,(exp_id,))\n",
        "    self.conn.commit()\n",
        "    print(f\"Expense {exp_id} successfully deleted.\")\n",
        "\n",
        "# Method to show expense:\n",
        "  def get_expense(self, exp_id=None):\n",
        "    if exp_id is not None:\n",
        "        cursor = self.conn.execute(\"SELECT id, amount, desc, category, date FROM expenses WHERE id=?\", (exp_id,))\n",
        "    else:\n",
        "        cursor = self.conn.execute(\"SELECT id, amount, desc, category, date FROM expenses\")\n",
        "\n",
        "    results = cursor.fetchall()\n",
        "\n",
        "    if results:\n",
        "        expense = []\n",
        "        for result in results:\n",
        "            expense.append({\n",
        "                'id': result[0],\n",
        "                'amount': result[1],\n",
        "                'desc': result[2],\n",
        "                'category': result[3],\n",
        "                'date': result[4],\n",
        "            })\n",
        "        return expense\n",
        "\n",
        "    return None\n",
        "\n",
        "# Method to get monthly expenses:\n",
        "  def get_monthly_expense(self):\n",
        "    cursor = self.conn.execute(\"SELECT strftime('%m', date) AS month, category, SUM(amount) FROM expenses GROUP BY month, category\")\n",
        "    results = cursor.fetchall()\n",
        "\n",
        "    if results:\n",
        "      monthly_expense = {}\n",
        "      for result in results:\n",
        "        month_number = int(result[0])\n",
        "        category = result[1]\n",
        "        total_amount = result[2]\n",
        "\n",
        "        month_names = {\n",
        "          1: 'January',\n",
        "          2: 'February',\n",
        "          3: 'March',\n",
        "          4: 'April',\n",
        "          5: 'May',\n",
        "          6: 'June',\n",
        "          7: 'July',\n",
        "          8: 'August',\n",
        "          9: 'September',\n",
        "          10: 'October',\n",
        "          11: 'November',\n",
        "          12: 'December'\n",
        "}\n",
        "        month_name = month_names.get(month_number)\n",
        "        if month_name not in monthly_expense:\n",
        "            monthly_expense[month_name] = {}\n",
        "\n",
        "        monthly_expense[month_name][category] =  total_amount\n",
        "\n",
        "      return monthly_expense\n",
        "\n",
        "    return None\n",
        "\n",
        "# Method to clear data:\n",
        "  def clear_db(self):\n",
        "        query = \"DELETE FROM expenses\"\n",
        "        self.conn.execute(query)\n",
        "        self.conn.commit()\n",
        "        print(\"All expenses cleared.\")"
      ]
    },
