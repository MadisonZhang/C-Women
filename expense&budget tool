    {
      "cell_type": "code",
      "source": [
        "#Bar chart: expense vs budgets:\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "monthly_budgets = budget_manager.get_budget()\n",
        "monthly_expenses = expense_manager.get_monthly_expense()\n",
        "\n",
        "months = list(monthly_expenses.keys())\n",
        "month1 = months[0]\n",
        "month2 = months[1]\n",
        "\n",
        "def get_category_budgets_expenses(month):\n",
        "    categories = []\n",
        "    expenses = []\n",
        "    budgets = []\n",
        "    for category, total_amount in monthly_expenses[month].items():\n",
        "        categories.append(category)\n",
        "        expenses.append(total_amount)\n",
        "        if category in monthly_budgets[month]:\n",
        "            budgets.append(monthly_budgets[month][category])\n",
        "        else:\n",
        "            budgets.append(0)\n",
        "\n",
        "    for category in monthly_budgets[month]:\n",
        "        if category not in categories:\n",
        "            categories.append(category)\n",
        "            expenses.append(0)\n",
        "            budgets.append(monthly_budgets[month][category])\n",
        "\n",
        "    return categories, expenses, budgets\n",
        "\n",
        "Ycategories, Yvals, Ybudgets = get_category_budgets_expenses(month1)\n",
        "Zcategories, Zvals, Zbudgets = get_category_budgets_expenses(month2)\n",
        "\n",
        "X_axis = np.arange(len(Ycategories))\n",
        "\n",
        "bar_width = 0.4  # Width of each bar\n",
        "bar_offset = bar_width / 2  # Offset for positioning the bars side by side\n",
        "\n",
        "# Define color scheme for expenses and budgets\n",
        "expense_color = 'tab:blue'\n",
        "budget_color = 'tab:orange'\n",
        "\n",
        "# Create a figure with two subplots arranged vertically\n",
        "fig, axes = plt.subplots(2, 1, figsize=(8, 12))\n",
        "\n",
        "# Plotting bar chart for month 1\n",
        "axes[0].bar(X_axis, Ybudgets, bar_width, label=f\"{month1} Budget\", color=budget_color)\n",
        "axes[0].bar(X_axis, Yvals, bar_width, label=f\"{month1} Expense\", color=expense_color)\n",
        "axes[0].set_xticks(X_axis)\n",
        "axes[0].set_xticklabels(Ycategories, rotation=45)\n",
        "axes[0].set_xlabel(\"Category\")\n",
        "axes[0].set_ylabel(\"Amount ($)\")\n",
        "axes[0].set_title(f\"Monthly Expense and Budget - {month1}\")\n",
        "axes[0].legend()\n",
        "\n",
        "# Plotting bar chart for month 2\n",
        "axes[1].bar(X_axis, Zbudgets, bar_width, label=f\"{month2} Budget\", color=budget_color)\n",
        "axes[1].bar(X_axis, Zvals, bar_width, label=f\"{month2} Expense\", color=expense_color)\n",
        "axes[1].set_xticks(X_axis)\n",
        "axes[1].set_xticklabels(Zcategories, rotation=45)\n",
        "axes[1].set_xlabel(\"Category\")\n",
        "axes[1].set_ylabel(\"Amount ($)\")\n",
        "axes[1].set_title(f\"Monthly Expense and Budget - {month2}\")\n",
        "axes[1].legend()\n",
        "\n",
        "plt.tight_layout()  # Adjust spacing between subplots\n",
        "\n",
        "plt.show()"
      ],
