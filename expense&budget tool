        "#Main code:\n",
        "budget_db_file = 'budget.db'\n",
        "exp_db_file = 'expense.db'\n",
        "budget_manager = Budget_Manager(budget_db_file)\n",
        "expense_manager = Exp_Manager(exp_db_file)\n",
        "\n",
        "#Add and save sample budgets\n",
        "print(\"*************Add budgets**************\")\n",
